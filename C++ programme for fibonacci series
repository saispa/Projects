#include<iostream>
using namespace std()
{ int f1=0;
int f2=1;
int n,i;
int next;
cout<<"Enter the number upto which you would like to see the series"<<endl;
cin>>n;
for(i=0;i<n;i++)
{
if(n<=1)
{ 
if(n==0)
{ cout<<f2; }
else 
{ cout<<f2;
cout<<f2; 
}
}
else
{
next= f1+f2;
f1=f2;
f2= next;
cout<<next;
}
}
return 0;
}
